<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>rpm.ac</path>

  <animation>
    <object-name>fond</object-name>
    <object-name>visses</object-name>
    <object-name>face</object-name>
    <object-name>needle</object-name>

    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>glass.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>needle</object-name>
    <emission>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/engines/engine[0]/rpm</property>
    <interpolation>
      <entry><ind>    0 </ind><dep>   0 </dep></entry>
      <entry><ind>  600 </ind><dep>  20 </dep></entry>
      <entry><ind> 1000 </ind><dep>  57 </dep></entry>
      <entry><ind> 1400 </ind><dep>  87 </dep></entry>
      <entry><ind> 1800 </ind><dep> 147 </dep></entry>
      <entry><ind> 2200 </ind><dep> 208 </dep></entry>
      <entry><ind> 2600 </ind><dep> 238 </dep></entry>
      <entry><ind> 3000 </ind><dep> 298 </dep></entry>
      <entry><ind> 3400 </ind><dep> 328 </dep></entry>
      <entry><ind> 3600 </ind><dep> 339 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>


